{
  "name": "Helper",
  "version": "1.0.0",
  "description": "✨ The only bot you will ever need!",
  "main": "dist/shard.js",
  "exports": [
    "./dist/shard.js"
  ],
  "scripts": {
    "dev": "ts-node-dev --rs --cls src/index.ts",
    "build": "tsc --project tsconfig.json",
    "prod": "node --enable-source-maps --experimental-specifier-resolution=node dist/shard.js",
    "build:prod": "npm run build && npm run prod",
    "repo:update": "git add . && git commit -m \"✨ Update finshed, please check the latest file in updates folder\" && git push helper && echo \"ShortCut: https://github.com/Saigeie/Helper\""
  },
  "keywords": [
    "discord.js",
    "bot",
    "typescript",
    "mongoose",
    "mongodb",
    "discord bot"
  ],
  "author": "Saige",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/humanize-duration": "^3.27.1",
    "@types/winston": "^2.4.4",
    "axios": "^0.26.1",
    "body-parser": "^1.19.2",
    "canvas": "^2.9.1",
    "captcha-canvas": "^3.1.0",
    "chalk": "^4.1.2",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.3",
    "express-rate-limit": "^6.3.0",
    "humanize-duration": "^3.27.1",
    "mongoose": "^6.2.8",
    "node-fetch": "^2.6.1",
    "passport": "^0.5.2",
    "passport-discord": "^0.1.4",
    "rate-limit-mongo": "^2.3.2",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
